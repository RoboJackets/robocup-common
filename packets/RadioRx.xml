<struct typename="RadioRx">
	<!-- Packet going out from radio program to control program -->
	
	<member type="uint64" name="timestamp" />
	<member type="uint8" name="board_id" />
	<member type="float" name="rssi" />
	<member type="float" name="battery" />
	
	<!-- ball sensor -->
	<member type="bool" name="ball" />
	
	<!-- True if the kicker is fully charged -->
	<member type="bool" name="charged" />
	
	<array type="bool" name="motorFault" size="5" />

	<array type="uint8" name="encoders" size="5" />
</struct>