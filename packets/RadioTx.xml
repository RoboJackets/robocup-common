<struct typename="RadioTx">
	<!-- Packet going out from control program to radio program -->
	
	<struct typename="Robot">
		<member type="uint8" name="board_id" />
		<member type="int8" name="roller" />
		<member type="uint8" name="kick" />
		<member type="bool" name="valid" />
		<member type="bool" name="useChipper" />
		
		<array type="int8" name="motors" size="4" />
	</struct>
		
	<!-- up to five robots can be controlled -->
	<array type="Robot" name="robots" size="5" />
	
	<!-- Which board should return reverse data.  Must be on [0, 15]. -->
	<member type="uint8" name="reverse_board_id" value="15"/>
</struct>
